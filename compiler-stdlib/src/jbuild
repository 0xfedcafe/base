(library
 ((name        caml)
  (public_name base.caml)
  (js_of_ocaml ())
  (preprocess no_preprocessing)))

;; Generate a file containing:
;;
;; module Array = Array
;; module List  = List
;; ...
;;
;; include Pervasives
(rule
 ((targets (caml.ml))
  (deps    (../gen/gen.exe))
  (action  (run ${<} -stdlib ${ocaml_where}/stdlib.cma -o ${@}))))

(enforce_style ())

(jbuild_version jane_street)
